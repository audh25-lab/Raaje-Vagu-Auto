{
  "name": "raaje-vagu-auto",
  "version": "1.0.0",
  "description": "Top-down GTA 1/2 style crime game based on RAAJE-VAGU-AUTO Bible",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html --public-url ./",
    "cap-init": "npx cap init com.raajevaguauto.app 'RAAJE-VAGU-AUTO'",
    "cap-add-android": "npx cap add android",
    "cap-add-ios": "npx cap add ios",
    "cap-sync": "npx cap sync",
    "cap-open-android": "npx cap open android",
    "cap-open-ios": "npx cap open ios"
  },
  "dependencies": {
    "phaser": "^3.80.1"
  },
  "devDependencies": {
    "@capacitor/android": "^6.1.2",
    "@capacitor/cli": "^6.1.2",
    "@capacitor/core": "^6.1.2",
    "@capacitor/ios": "^6.1.2",
    "@types/node": "^22.5.1",
    "parcel": "^2.12.0",
    "typescript": "^5.6.2"
  }
}